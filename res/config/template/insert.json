{
    "validation": {
        "start_date": {
            "title"   : "시작일",
            "required": true,
            "filter"  : "chkDateformat"
        },
        "end_date": {
            "title"   : "종료일",
            "required": true,
            "filter"  : "chkDateformat"
        },
        "title": {
            "title"   : "제목",
            "required": true,
            "filter"  : "chkNull"
        },
        "link": {
            "title"   : "이동할 주소",
            "required": true,
            "filter"  : "chkLinkurl"
        },
        "extract_id": {
            "title"   : "첨부파일토큰",
            "required": true,
            "filter"  : "chkEngNumUnderline"
        }
    },

    "authority" : "{__FUNC__.chk_authority}({_DEFINE._AUTH_SUPERADMIN_LEVEL})",

    "model": {
    },

    "data":{
        "insert" : {
            "query"  : null,
            "query_type": "insert",
            "scheme" : {
                "start_date" : {
                    "value" : "{__REQ__.start_date}"
                },
                "end_date" : {
                    "value" : "{__REQ__.end_date}"
                },
                "title" : {
                    "value" : "{__REQ__.title}"
                },
                "link" : {
                    "value" : "{__REQ__.link}"
                },
                "signdate" : {
                    "value" : "{__FUNC__.time}()"
                },
                "extract_id":{
                    "type" : "file",
                    "table" : "{__UPLOADABLE__.table}",
                    "image_size" : "{__UPLOADABLE__.image_size}",
                    "value" : "{__REQ__.extract_id}"
                }
            },
            "params" : {
                "table"  : "{__TABLES__.popup}",
                "where"  : null
            }
        }
    },

    "alarm" : {
        "state" : false,
        "msg" : "{__REQ__.title}",
        "params" : "{__TABLES__.notice}",
        "condition" : {
            "headline" : {
                "condi": " = ",
                "value"    : "y"
            }
        },
        "send_option" : "all",
        "push" : false
    },

    "output": {
        "result": "true",
        "msg": "{__SYSMSG__.v_insert}"
    }
}